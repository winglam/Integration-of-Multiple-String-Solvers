String(v);
FixedLength(v, 1);
CFG(SQL, SelectStmt | UpdateStmt | DeleteStmt);
CFG(DeleteStmt, Delete From Id WhereCondOpt);
CFG(UpdateStmt, Update Id Set AssignmentList WhereCondOpt);
CFG(SelectStmt, Select ColumnSet From IdList WhereCondOpt OrderByOpt);
CFG(AssignmentList, Assignment | Assignment Comma AssignmentList);
CFG(Assignment, Id Eq Val);
CFG(ColumnSet, Star | IdList);
CFG(IdList, Id | Id Comma IdList);
CFG(OrderByOpt, OrderBy |);
CFG(OrderBy, Order By Id);
CFG(WhereCondOpt, WhereCond |);
CFG(WhereCond, Where Cond);
CFG(Cond, Val Eq Val | Cond Or Cond | Cond And Cond);
CFG(Val, Id | Num | Quote Id Quote | Quote Quote | Quote Num Quote);
CFG(Or, "O");
CFG(And, "A");
CFG(Select, "S");
CFG(Star, "R");
CFG(From, "F");
CFG(Id, "i");
CFG(Num, "n");
CFG(Eq, "Q");
CFG(Where, "W");
CFG(Comma, "C");
CFG(Quote, "T");
CFG(Update, "U");
CFG(Set, "E");
CFG(Delete, "D");
CFG(Order, "X");
CFG(By, "B");
Regex(sqlLimit, Fix(SQL, 48));
StringDecl(q, ConcatString("UiEiQT", ConcatString(v, "TCiQTnTCiQTnTCiQTnTCiQTnTCiQiCiQTnTWiQTnT")));
Contains(q, "OiQi");
AssertIn(q, sqlLimit);
